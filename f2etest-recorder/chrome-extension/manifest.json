{
   "manifest_version": 2,
   "name": "F2etest recorder",
   "description": "F2etest test recorder",
   "version": "1.0.0",
   "browser_action": {
      "default_icon": "img/icon.png",
      "default_title": "F2etest recorder"
   },
   "content_scripts": [{
      "matches": [ "http://*/*", "https://*/*" ],
      "all_frames": true,
      "match_about_blank": true,
      "run_at": "document_start",
      "js": ["js/faker.js", "js/foreground.js"]
   }],
   "web_accessible_resources": [
      "js/foreground.js",
      "img/hover.png",
      "img/expect.png",
      "img/end.png",
      "img/ok.png",
      "img/cancel.png",
      "img/vars.png",
      "img/success.png",
      "img/fail.png"
   ],
   "background": {
      "scripts": [ "js/background.js" ]
   },
   "icons": {
      "16": "img/icon.png",
      "64": "img/icon-64.png",
      "128": "img/icon-128.png"
   },
   "permissions": ["tabs", "webNavigation", "\u003Call_urls\u003E", "notifications"]
}
